<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="nc7.mapp.dao.CommentLikeDao">

  <resultMap type="nc7.mapp.vo.CommentLike" id="commentLikeMap">
    <id column="comments_no" property="commentNo"/>
    <id column="users_no" property="userNo"/>
  </resultMap>

  <insert id="insert" parameterType="nc7.mapp.vo.CommentLike">
    INSERT INTO comments_like(comments_no, users_no)
    VALUES (#{commentNo}, #{userNo})
  </insert>

  <delete id="delete" parameterType="java.util.Map">
    DELETE FROM comments_like
    WHERE comments_no = #{commentNo}
    AND users_no = #{userNo}
  </delete>

</mapper>
